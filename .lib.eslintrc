{
  "extends": ["eslint:recommended", "plugin:node/recommended"],
  "env": { "browser": true },
  "plugins": ["compat"],
  "rules": {
    "no-unsafe-finally": "off",
    "no-func-assign": "off",
    "no-unused-vars": "off",
    "no-constant-condition": "off",
    "no-fallthrough": "off"
  },
  "settings": {
    "polyfills": []
  }
}
